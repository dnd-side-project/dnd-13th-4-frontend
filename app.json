{
  "expo": {
    "name": "Wini",
    "slug": "dnd-13th-4-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "com.kirikiri.wini",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "entitlements": {
        "aps-environment": "production"
      },
      "googleServicesFile": "./GoogleService-Info.plist",
      "useFrameworks": "static",
      "supportsTablet": true,
      "bundleIdentifier": "com.kirikiri.wini",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "appleTeamId": "MRNM55WK54"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#5BA4FA"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.kirikiri.wini",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 134,
          "resizeMode": "contain",
          "backgroundColor": "#5BA4FA"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "fonts": [
      {
        "asset": "./assets/fonts/Pretendard-Regular.ttf",
        "family": "Pretendard"
      },
      {
        "asset": "./assets/fonts/Pretendard-Medium.ttf",
        "family": "Pretendard",
        "weight": "500"
      },
      {
        "asset": "./assets/fonts/Pretendard-SemiBold.ttf",
        "family": "Pretendard",
        "weight": "600"
      },
      {
        "asset": "./assets/fonts/Pretendard-Bold.ttf",
        "family": "Pretendard",
        "weight": "700"
      }
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d5d172fd-934f-4e97-9dc8-45f56d83980a"
      }
    },
    "owner": "wini_kirikiri"
  }
}
